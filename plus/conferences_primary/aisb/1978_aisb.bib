@inproceedings{10.5555/3017065.3017066,
 author       = {Anzai, Yuichiro},
 crossref     = {78:_proceed_aisb_gi_confer_artif_intel_},
 title        = {How One Learns Strategies: Processes and Representation of Strategy Acquisition},
 year         = {1978},
 abstract     = {A general theory is presented for the process and representation of strategy acquisition by human and artificial intelligence systems. The process is characterized by inductive generalization and organization of new rules through successive construction of new problem spaces. Various subtasks involved in the process can be naturally represented by subroutines in a hierarchical adaptive production system (HAPS), which is proposed as a representational scheme for strategy acquisition. HAPS has capabilities for subroutine calls and creating new productions in any subroutine. Results from strategy acquisition in the Tower of Hanoi puzzle by a HAPS program are given.},
 booktitle    = {Proceedings of the 1978 AISB/GI Conference on Artificial Intelligence},
 pages        = {1-14},
 numpages     = {14},
 keywords     = {production systems, problem solving, strategy acquisition, rule learning},
 series       = {AISB/GI'78}
}

@inproceedings{10.5555/3017065.3017067,
 author       = {Barstow, David R.},
 crossref     = {78:_proceed_aisb_gi_confer_artif_intel_},
 title        = {Experience with a Refinement Paradigm in a Knowledge-Based Automatic Programming System},
 year         = {1978},
 abstract     = {PECOS is a knowledge-based system that constructs concrete implementations of abstract algorithms in the domain of symbolic programming. The rules in PECOS's knowledge base deal explicitly with many details of symbolic programming, including intermediate-level constructs and certain design decisions. Programs are synthesized by gradually refining the original specification into a program in the target language. By applying the refinement rules in its knowledge base, PECOS constructs a refinement tree: the root is the abstract specification, each leaf is an alternative implementation, and each path is a different sequence of rule applications. Thus, PECOS can construct a variety of different implementations for the same abstract specification.},
 booktitle    = {Proceedings of the 1978 AISB/GI Conference on Artificial Intelligence},
 pages        = {15-21},
 numpages     = {7},
 series       = {AISB/GI'78}
}

@inproceedings{10.5555/3017065.3017068,
 author       = {Bibel, W.},
 crossref     = {78:_proceed_aisb_gi_confer_artif_intel_},
 title        = {On Strategies for the Synthesis of Algorithms},
 year         = {1978},
 abstract     = {A number of strategies for the construction of algorithms from a given input-output specification of a problem are presented, Their application is illustrated by a detailed synthesis of the usual MAXIMUM algorithm and Hoares FIND algorithm.},
 booktitle    = {Proceedings of the 1978 AISB/GI Conference on Artificial Intelligence},
