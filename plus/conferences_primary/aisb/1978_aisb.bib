@inproceedings{10.5555/3017065.3017112,
 author       = {Suzman, P. L.},
 crossref     = {78:_proceed_aisb_gi_confer_artif_intel_},
 title        = {Common-Sense Representation},
 year         = {1978},
 abstract     = {Some problems with existing formal representation languages are discussed, and an alternative semantics is proposed that attempts to build in a particular view of the world. This is essentially that the world consists of objects whose properties are determined by the properties of their parts. The semantics also makes provision for predicates to have intensions as well as extensions, so they can be reasoned about. The implications of the semantics are discussed with particular reference to the problems of representing change, where it is contrasted to the approach taken in STRIPS and the Situational Calculus.},
 booktitle    = {Proceedings of the 1978 AISB/GI Conference on Artificial Intelligence},
 pages        = {345-350},
 numpages     = {6},
 keywords     = {semantics, representational language, representation of change},
 series       = {AISB/GI'78}
}

