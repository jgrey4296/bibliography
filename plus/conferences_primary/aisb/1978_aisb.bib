}

@inproceedings{10.5555/3017065.3017069,
 author       = {Birch, P.},
 crossref     = {78:_proceed_aisb_gi_confer_artif_intel_},
 title        = {A (Self-Adapting) Network for Recognition of Visual Structure},
 year         = {1978},
 abstract     = {A system is presented that recognises emerging scene structures as a picture is interpreted. Emphasis is placed on using local gross features (which are cheap to compute) to guide the building of an interpretation. When this results in ambiguity, more global considerations can be used. Finer features (more expensive to compute) can also be utilised when present.},
 booktitle    = {Proceedings of the 1978 AISB/GI Conference on Artificial Intelligence},
 pages        = {28-32},
 numpages     = {5},
 keywords     = {self-adapting, alternative interpretations, ambiguities, intermediate descriptions, 'Gestalt', perception, recognition},
 series       = {AISB/GI'78}
}

@inproceedings{10.5555/3017065.3017070,
 author       = {B\"{o}hm, H. P. and Fischer, H. L. and Raulefs, P.},
 crossref     = {78:_proceed_aisb_gi_confer_artif_intel_},
 title        = {Dialogues in Actor Nets},
 year         = {1978},
 abstract     = {The actor society representation of knowledge is introduced. In a self-organizing society of actors, knowledge is distributed s.t. each individual actor has strictly local information. Although each actor exclusively tries to achieve its own private goal, it exhibits a communication behavior making the entire society form a well-orgaized system having global properties no individual actor is aware of. Actor societies are realized as networks of CSSA-actors /1/.},
 booktitle    = {Proceedings of the 1978 AISB/GI Conference on Artificial Intelligence},
 pages        = {33-34},
 numpages     = {2},
 series       = {AISB/GI'78}
}

@inproceedings{10.5555/3017065.3017071,
 author       = {Boley, Harold},
 crossref     = {78:_proceed_aisb_gi_confer_artif_intel_},
 title        = {The Fit Approach to AI Languages},
 year         = {1978},
 abstract     = {An overview of the AI language FIT is given, which attempts to unify, generalize, and advance a number of programming language concepts. Its central entities are fitters, a unification of functions, patterns, and variables, especially of predicate functions and typed variables. The latter are also viewed as new kinds of demons. Fitter definitions are assignments of bodies to complex variables viewable both as function application forms and as consequent theorem invocation patterns. Fitter computation and look-up are thus unified to fitter valuation.},
 booktitle    = {Proceedings of the 1978 AISB/GI Conference on Artificial Intelligence},
 pages        = {35-41},
 numpages     = {7},
 keywords     = {FIT, pattern matching, demons, types, complex variables, valuation, fitments, fitters, consequent theorems, AI languages},
 series       = {AISB/GI'78}
}

