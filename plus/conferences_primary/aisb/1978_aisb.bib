@inproceedings{10.5555/3017065.3017073,
 author       = {Brazdil, Pavel},
 crossref     = {78:_proceed_aisb_gi_confer_artif_intel_},
 title        = {Experimental Learning Model},
 year         = {1978},
 abstract     = {The aim of our work is to investigate how a relatively small set of rules can be transformed into a running program capable of solving a fairly wide variety of problems. We have written a program ELM (Experimental Learning Model) which modifies a given set of rules (clauses) as a result of its experience. The problems are chosen from the domain of school arithmetic/algebra and solved by one part of our system (with the help of the teacher). The solutions are analyzed and modifications to the existing system of rules are performed as a result.},
 booktitle    = {Proceedings of the 1978 AISB/GI Conference on Artificial Intelligence},
 pages        = {46-50},
 numpages     = {5},
 series       = {AISB/GI'78}
}

@inproceedings{10.5555/3017065.3017074,
 author       = {Brooks, M. J.},
 crossref     = {78:_proceed_aisb_gi_confer_artif_intel_},
 title        = {Understanding the Image Forming Process},
 year         = {1978},
 abstract     = {Relatively little work has been done to try to understand the image forming process. It is contended here that this situation must change if vision programs are to attain any level of sophistication. Some difficult problems are posed and, as a first step towards answering them, an attempt is made to understand the effects of a light-source which is a 'sky' rather than the traditionally adopted point source.},
 booktitle    = {Proceedings of the 1978 AISB/GI Conference on Artificial Intelligence},
 pages        = {51-55},
 numpages     = {5},
 keywords     = {point and sky light sources, analytic approach, image forming process, reflectivity function},
 series       = {AISB/GI'78}
}

@inproceedings{10.5555/3017065.3017075,
 author       = {Brown, F. M.},
 crossref     = {78:_proceed_aisb_gi_confer_artif_intel_},
 title        = {A Sequent Cadculus for Modal Quantificational Logic},
 year         = {1978},
 abstract     = {We give an axiomization of a modal quantificational logic which captures the notion of logical truth. This modal logic is stronger than S5. Next we describe a sequent calculus for this modal logic, and show that it is complete. Finally we describe some potential applications of Modal Logic to Artificial Intelligence.},
 booktitle    = {Proceedings of the 1978 AISB/GI Conference on Artificial Intelligence},
 pages        = {56-65},
 numpages     = {10},
 series       = {AISB/GI'78}
}

