@inproceedings{10.5555/3017065.3017078,
 author       = {Champeaux, Dennis de},
 crossref     = {78:_proceed_aisb_gi_confer_artif_intel_},
 title        = {A Theorem Prover Dating a Semantic Network},
 year         = {1978},
 abstract     = {Ordinary predicate calculus (p.c.) is equiped with sorts, an extension of types, allowing a concise formulation of problems. A semantic network can easily provide the sort-part of the input of a theorem prover based on a sorted p.c. Experimental results are reported of such a theorem prover.},
 booktitle    = {Proceedings of the 1978 AISB/GI Conference on Artificial Intelligence},
 pages        = {82-92},
 numpages     = {11},
 keywords     = {typed predicate calculus, n1-resolution, sorts, semantic networks, theorem proving},
