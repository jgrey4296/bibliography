 tags         = {__untagged},
 abstract     = {In the paper a qualitative approach to the problem of aggregation of information is discussed. The method is based on the notion of fuzzy attribute model of the aggregation problem in which information is represented in the form of fuzzy constraints on the values of attributes. The aggregate is built by carrying out logical inference on the knowledge base which represents the decision maker preferences in the form of fuzzy logical dependencies between attributes.},
 issn         = {1561-4042},
