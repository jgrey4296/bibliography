@article{SeiditaCossentinoHilaireGaudGallandKoukamGaglio2010_12,
 author       = {Seidita, Valeria and Cossentino, Massimo and Hilaire, Vincent and Gaud, Nicolas and Galland, St\'ephane and Koukam, Abderrafiaa and Gaglio, Salvatore},
 title        = {The Metamodel},
 subtitle     = {A Starting Point for Design Processes Construction},
 year         = {2010},
 journal      = {International Journal of Software Engineering and Knowledge Engineering (IJSEKE)},
 tags         = {__untagged},
 abstract     = {The construction of ad-hoc design processes following the Situational Method Engineering (SME) paradigm is currently carried out by adopting a set of phases for which, until now, no well defined techniques and guidelines have been established. The consequence is that organizations are very dependent on method designers' skills. In this paper, we propose an approach based on SME for constructing customized agent oriented design processes. Our approach adopts the metamodel as the most important factor leading to the selection and assembly of method fragments and an algorithm for establishing the instantiation order of metamodel elements. The algorithm makes the proposed approach repeatable and usable even by not very skilled personnel, thus proposing an improvement to the actual situation. The proposed approach and the algorithm are also experimented through the construction of a design process (ASPECS) for developing dynamic hierarchical societies of agents. The approach we created is general enough to be applied in other development contexts (not only agent-oriented).},
 doi          = {10.1142/S0218194010004785},
 keywords     = {Metamodel; design process; multi-agent system; situational method engineering},
 month        = {jun},
 number       = {4},
 pages        = {575{\textendash}608},
 publisher    = {World Scientific Publishing Company},
 url          = {http://www.worldscinet.com/ijseke/20/2004/S0218194010004785.html},
 volume       = {20},
}

