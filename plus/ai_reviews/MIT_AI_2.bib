@techreport{AIM-366,
 author       = {Winston, Patrick H.},
 title        = {Proposal to the Advanced Research Projects Agency},
 year         = {1976},
 institution  = {MIT Artificial Intelligence Laboratory},
 tags         = {__untagged},
 abstract     = {This is the substance of a proposal submitted in June, 1975, for research in the areas of large data bases and intelligent terminals, applications of machine vision and manipulation, basic studies in Artificial Intelligence, and LISP machine development.},
