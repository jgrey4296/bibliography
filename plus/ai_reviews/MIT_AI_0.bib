@techreport{AIM-1227,
 author       = {Brooks, Rodney A.},
 title        = {The Behavior Language; User's Guide},
 year         = {1990},
 institution  = {MIT Artificial Intelligence Laboratory},
 tags         = {__untagged},
 abstract     = {The Behavior Language is a rule-based real- time parallel robot programming language originally based on ideas from [Brooks 86], [Connell 89], and [Maes 89]. It compiles into a modified and extended version of the subsumption architecture [Brooks 86] and thus has backends for a number of processors including the Motorola 68000 and 68HCll, the Hitachi 6301, and Common Lisp. Behaviors are groups of rules which are activatable by a number of different schemes. There are no shared data structures across behaviors, but instead all communication is by explicit message passing. All rules are assumed to run in parallel and asynchronously. It includes the earlier notions of inhibition and suppression, along with a number of mechanisms for spreading of activation.},
 month        = {apr~6},
 number       = {AIM-1227},
 url          = {ftp://publications.ai.mit.edu/ai-publications/1000-1499/AIM-1227.ps; ftp://publications.ai.mit.edu/ai-publications/pdf/AIM-1227.pdf},
}

@techreport{AIM-123,
 author       = {Minsky, Marvin and Papert, Seymour},
 title        = {Computer Tracking of Eye Motions},
 year         = {1967},
 institution  = {MIT Artificial Intelligence Laboratory},
 tags         = {__untagged},
 abstract     = {This memo is to explain why the Artificial Intelligence group of Project MAC is developing methods for on-line tracking of human eye movements. It also gives a brief resume of results to date and the next steps.},
 month        = {mar~19},
 number       = {AIM-123},
