@techreport{agent-k,
 author       = {Davies, Winton and Edwards, Peter},
 title        = {Agent-K},
 subtitle     = {An Integration of Agent-Oriented Programming and the Knowledge Query     Manipulation Language},
 year         = {1994},
 institution  = {Department of Computing Science, University of Aberdeen},
 tags         = {__untagged},
 abstract     = {This report describes a synthesis of two well-known agent paradigms: Agent-Oriented Programming, Shoham (1990), and the Knowledge Query     Manipulation Language, Finin (1993). The initial implementation of AOP, Agent-0, is a simple language for specifying agent behaviour. KQML provides a standard language for inter-agent communication. Our integration (which we have called Agent-K) demonstrates that Agent-0 and KQML are highly compatible. Agent-K provides the possibility of inter-operable (or open) software agents, that can communicate via KQML and which are programmed using the AOP approach.},
 address      = {Aberdeen, Scotland, UK},
 number       = {AUCS/TR9406},
 url          = {ftp://ftp.csd.abdn.ac.uk/pub/reports/tr9406.ps},
}

