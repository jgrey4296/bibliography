 editor       = {Etzioni, O.},
 title        = {An Experiment in the Design of Software Agents},
 year         = {1994},
 booktitle    = {etz-soft-agents},
 tags         = {__untagged},
 abstract     = {We describe a bottom-up approach to the design of software agents. We built and tested an agent system that addresses the real-world problem of han-dling the activities involved in scheduling a visitor to our laboratory. The system employs both task-specific and user-centered agents, and communicates with users using both email and a graphicalinterface. This experiment has helped us to identify crucial requirements in the successful deployment of software agents, including issues of reliability, security, and ease of use. The architecture we de-veloped to meet these requirements is flexible and extensible, and is guiding our current research on principles of agent design.},
