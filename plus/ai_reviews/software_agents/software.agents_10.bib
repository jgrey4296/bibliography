@techreport{Dartmouth:TR2001-411,
 author       = {Grimstrup, Arne and Gray, Robert S. and Kotz, David and Cowin, Thomas and Hill, Greg and Suri, Niranjan and Chacon, Daria and Hofmann, Martin},
 title        = {Write Once, Move Anywhere},
 subtitle     = {Toward Dynamic Interoperability of Mobile Agent Systems},
 year         = {2001},
 institution  = {Dartmouth College, Computer Science},
 tags         = {__untagged},
 abstract     = {Mobile agents are an increasingly popular paradigm, and in recent years there has been a proliferation of mobile-agent systems. These systems are, however, largely incompatible with each other. In particular, agents cannot migrate to a host that runs a different mobile-agent system. Prior approaches to interoperability have tried to force agents to use a common API, and so far none have succeeded. Our goal, summarized in the catch phrase {\textquotedblleft}Write Once, Move Anywhere,{\textquotedblright} led to our efforts to develop mechanisms that support dynamic runtime interoperability of mobile-agent systems. This paper describes the Grid Mobile-Agent System, which allows agents to migrate to different mobile-agent systems.},
 address      = {Hanover, NH},
 comment      = {Revised July 25, 2001.},
 month        = {jul},
 number       = {TR2001-411},
 url          = {ftp://ftp.cs.dartmouth.edu/TR/TR2001-411.ps.Z},
}

