@inproceedings{esaw00--04,
 author       = {Castelfranchi, Cristiano},
 editor       = {esaw00-editor},
 title        = {Engineering Social Order},
 year         = {2000},
 booktitle    = {esaw00-booktitle},
 tags         = {__untagged},
 abstract     = {Social Order becomes a major problem in MAS and in computer mediated human interaction. After explaining the notions of Social Order and Social Control, I claim that there are multiple and complementary approaches to Social Order and to its engineering: all of them must be exploited. In computer science one try to solve this problem by rigid formalisation and rules, constraining infrastructures, security devices, etc. I think that a more socially oriented approach is also needed. My point is that Social Control - and in particular decentralised and autonomous Social Control - will be one of the most effective approaches.},
 month        = {dec},
 pages        = {1{\textendash}18},
}

@inproceedings{esaw00--05,
 author       = {Parunak, H. Van Dyke and Brueckner, Sven and Sauter, John and Matthews, Robert S.},
 editor       = {esaw00-editor},
 title        = {Distinguishing Environmental and Agent Dynamics},
 subtitle     = {A Case Study in Abstraction and Alternate Modeling Technologies},
 year         = {2000},
 booktitle    = {esaw00-booktitle},
 tags         = {__untagged},
 abstract     = {Two factors can confound the interpretation of an MAS application or model. First, the MAS's dynamics interact in complex ways with those of its environment, and agent engineers need to distinguish the two. Second, {\textquotedblleft}mean field{\textquotedblright} approximations of the behavior of the system may be useful for qualitative examination of the dynamics, but can differ surprisingly from the behavior that emerges from the interactions of discrete agents. This paper examines these effects in the context of a project applying synthetic pheromones for agent-based control to a military air operations scenario.},
 month        = {dec},
 pages        = {19{\textendash}33},
 publisher    = {sv},
}

@inproceedings{esaw00--07,
