@article{Tolun:1998,
 author       = {Tolun, Mehmet R. and Alpaslan, Ferda and Abuzir, Yousef},
 title        = {An expert system for life insurance underwriting},
 year         = {1998},
 journal      = {New Review of Applied Expert Systems},
 institution  = {Faculty of Engineering, Eastern Mediterranean University,Gazimagusa,T.R.N.C., Via Mersin 10, TURKEY +90-392-6301381 (voice) , +90-392-3651217 (fax) , mehmet.tolun@emu.edu.tr,http://mesarya.eng.emu.edu.tr/~tolun/},
 tags         = {__untagged},
 abstract     = {This article describes the development and implementation of a life insurance underwriting expert system using a shell. The feasibility of utilising an expert system in this domain was examined and determined to be practical, and development of a prototype, UES, was initiated. The objective of the system is to determine the insurability of an applicant. UES extracts data from actuarial and medical tables and applicant related data through a user-friendly dialogue. Some of this information is processed outside the shell, Personal Consultant Plus. We also describe shell's external interfacing facilities for programs written in Pascal and PC Scheme to calculate premiums.},
 pages        = {3{\textendash}17},
 url          = {http://www.qub.ac.uk/mgt/alans/abst4.htm},
 volume       = {4},
}

@inproceedings{toms:1996:pitleotAPES,
 author       = {Toms, Mona L. and Cavallaro, Joseph J. and Cone, Scott M. and Moore, Frank W. and Garcia, Oscar N. and Gonzalez-Garcia, Airam},
 editor       = {Storms, Penny},
 title        = {Pilot-in-the-Loop Evaluation of the Approach Procedures Expert System (APES)},
 year         = {1996},
 booktitle    = {Proceedings, Third Annual Symposium on Human Interaction with Complex Systems (HICS'96)},
 tags         = {__untagged},
 abstract     = {The Advanced Cockpits Branch of Wright Laboratory (WL/FIGP) conducted a study to assess the utility of a prototype decision aid to support the pilot's use of electronic approach plates and assist the pilot in flying instrument approaches. The prototype decision aid system that was evaluated in this study is the Approach Procedures Expert System (APES). The study consisted of 16 pilots flying a series of instrument approaches in a cockpit simulator. Performance data, subjective workload, situational awareness and acceptability ratings were collected. The results showed that the APES reduced pilot workload, increased situational awareness, and improved flight performance compared to flying approaches without the decision aid. Refinements to the APES algorithm and pilot-vehicle interface were indicated. Implications for further research are discussed.},
 address      = {Department of Computer Science     Engineering, Wright State University, Dayton, Ohio, USA},
 keywords     = {expert systems, decision aiding, approach procedures, situational awareness, workload, pilot-vehicle interface},
 month        = {25-28aug},
 pages        = {54{\textendash}61},
 publisher    = {IEEE Computer Society Press},
}

