@techreport{ra0:02,
 author       = {Rao, Anand S.},
 title        = {Dynamics of belief systems},
 subtitle     = {A philosophical, logical, and ai perspective},
 year         = {1989},
 institution  = {Australian Artificial Intelligence Institute},
 tags         = {__untagged},
 abstract     = {Reasoning about the beliefs of an agent has played an important role in Philosophy, Logic, and Artificial Intelligence. A great deal of effort has been spent on analyzing and formalizing the static aspects of belief systems. Although the dynamic aspects of belief systems, or aspects relating to how beliefs change with time, has been recognised as being important, detailed work describing the dynamic aspects of belief systems has started emerging only recently. This report examines the dynamic aspects of belief systems, also called belief revision, from a philosophical, logical, and AI perspective. It discusses two philosophical theories of belief revision, namely the coherence and foundational theories. Unlike most philosophical works which treat these as two opposing theories, we view the foundational theory as an extension of the coherence theory of belief revision. Coherence theory is based on the intuitive principles of minimal change and maximal coherence, whereas foundational theory is based on the above two principles together with the foundational thesis, which states that all beliefs are justified beliefs. By providing a possible-worlds semantics, axiomatization, and soundness and completeness results for both the theories, the report presents a detailed and formal account of belief revision that has been lacking in most earlier work in this area. This formal theory is then used to reason about actions. The resulting theory provides an elegant solution to some of the important philosophical problems in AI, namely the conceptual frame and ramification problems. It also facilitates reasoning in dynamic situations where the world changes during the execution of an action. The principles of minimal change and maximal coherence seem to unify belief revision and reasoning about actions and may form a fundamental core for reasoning about other dynamic processes that involve change.},
 address      = {Melbourne, Australia},
 month        = {jul},
 number       = {02},
 url          = {ftp://www.aaii.com.au/pub/aaii-technotes/technote02.ps.gz},
}

