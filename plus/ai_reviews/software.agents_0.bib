@inproceedings{levesque:90,
 author       = {Levesque, H. J. and Cohen, P. R. and Nunez, J. H. T.},
 editor       = {Dietterich, Thomas and Swartout, William},
 title        = {On acting together},
 year         = {1990},
 booktitle    = {Proceedings of the 8th National Conference on Artificial Intelligence (AAAI-90)},
 tags         = {__untagged},
 abstract     = {Joint action by a team does not consist merely of simultaneous and coordinated individual actions; to act together, a team must be aware of and care about the status of the group effort as a whole. We present a formal definition of what it could mean for a group to jointly commit to a common goal, and explore how these joint commitments relate to the individual commitments of the team members. We then consider the case of joint intention, where the goal in question involves the team performing some action. In both cases, the theory is formulated in a logical language of belief, action, and time previously used to characterize individual commitment and intention. An important consequence of the theory is the types of communication among the team members that it predicts will often be necessary.},
 address      = {Boston, MA, USA},
 mohth        = {jul31~aug{\textendash}3~},
 pages        = {94{\textendash}99},
 publisher    = {aaaip},
}

@article{m-drt-90,
 author       = {Maes, P.},
 title        = {How to do the Right Thing},
 year         = {1990},
 journal      = {Connection Science Journal, Special Issue on Hybrid Systems},
 tags         = {__untagged},
 abstract     = {This paper presents a novel approach to the problem of action selection for an autonomous agent. An agent is viewed as a collection of competence modules. Action selection is modelled as an emergent property of an activation/inhibition dynamics among these modules. A concrete action selection algorithm is presented and a detailed account of the results is given. This algorithm combines characteristics of both traditional planners and reactive systems. It provides global parameters, which one can use to tune the action selection behavior along several criteria, such as goal orientedness versus situation orientedness, bias towards ongoing plans versus adaptivity, and sensitivity to goal conflicts and `thoughtfulness' versus speed.},
 url          = {http://agents.www.media.mit.edu/groups/agents/Publications/Pattie/consci/consci.ps},
 volume       = {1},
}

@inproceedings{Maes/Brooks:1990,
 author       = {Maes, Pattie and Brooks, Rodney A.},
 editor       = {Dietterich, Thomas and Swartout, William},
 title        = {Learning to co-ordinate behaviours},
 year         = {1990},
 booktitle    = {Proceedings of the 8th National Conference on Artificial Intelligence (AAAI-90)},
 tags         = {__untagged},
 abstract     = {Behaviour based decomposition of an autonomous robot into modules such as build-maps, explore, wander, avoid objects, is more useful than the classical decomposition which is perception, modelling, planning, task execution, motor control. An algorithm is described that allows a behaviour based robot to learn on the basis of positive and nagative feedback when to activate its behaviour s. In accordance with the philosophy of the behaviour based robots the algorithm is completely distributed. Each of the behaviours independently tries to find out :- 1. whether it is relevant (i.e. whether it is all correlated to a positive feedback.) 2. what the conditions are under which it maximises the probability of receiving the positive feedback and minimises the probability of receiving a negative feedback. The algorithm was tested on a 6 legged robot which had to learn how to co-ordinate its legs to walk forward. As the number of behaviours increase, the problem of control, co-ordination becomes increasingly complex. There is a complete distribution in the algorithm and there is no central learning component and each behaviour tries to learn when it should become active. The global search space for the robot with n behaviours and m binary perceptual conditions is $n*3^m$ as every behaviour has to learn an on,off and a don't care value for every perceptual condition. Relevance and reliability of a behaviour are also defined. Research is going on to make Genghis walk backward by adding a switch that inverts the movement feedback. Study of the properties of the algorithm by building a mathematical model of it is also a proposed research. - Mali 10/93},
 month        = {jul29~aug{\textendash}3~},
 pages        = {296{\textendash}802},
 volume       = {2},
}

