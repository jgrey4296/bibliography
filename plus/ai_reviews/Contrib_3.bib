 issn         = {1699-4574},
 number       = {6},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/3},
 volume       = {3},
}

@article{IECom30,
 author       = {L\'opez-Cuadrado, Javier and Fern\'andez, Joaqu{\i}́n S\'anchez},
 title        = {GenTAI},
 subtitle     = {Generador de tests adaptativos informatizados},
 year         = {2005},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {Este art{\i}́culo presenta una herramienta que permite la generaci\'on autom\'atica de Tests Adaptativos Informatizados experimentales o a peque\~na escala a partir de un banco de ́ems que est\'e calibrado seg\'un el modelo logt́́ico de 3 par\'ametros de la Teor{\'\i}d́e Respuesta al \'Item. GenTAI es un software amigable, de f\'acil uso, intuitivo, portable, escalable, altamente configurable y fuertemente documentado, que al mismo tiempo da respuesta a los objetivos de un test adaptativo. Otros sistemas similares pecan en la calidad de sus interfaces y en la forma de interactuar con el usuario. Como novedad, GenTAI incluye un m\'odulo que permite al usuario elaborar simulaciones para evaluar o comparar los errores de medida obtenidos tras la aplicaci\'on de los tests.},
 bdsk-url-1   = {http://www.adie.es/iecom/index.php/IECom/article/view/30},
