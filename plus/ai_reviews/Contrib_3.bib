 date-modified = {2009-03-02 20:07:20 +0100},
 issn         = {1699-4574},
 keywords     = {tests adaptativos informatizados; bancos de {\i}́tems; herramientas de evaluaci\'on.},
 number       = {2},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/30},
 volume       = {1},
}

@article{IECom31,
 author       = {Vera, Ra\'ul Aguilar and de Antonio Jim\'enez, Ang\'elica},
 title        = {Entrenamiento de equipos},
 subtitle     = {Una estrategia asistida por entornos virtuales inteligentes},
 year         = {2005},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {El uso de Entornos Virtuales Inteligentes (EVI's) como herramienta de apoyo a la educaci\'on representa un intento por ofrecer un modelo ecl\'ectico para los esquemas de formaci\'on que han asistido de manera independiente a los procesos de ense\~nanza y aprendizaje. En este trabajo se describe una estrategia de entrenamiento para peque\~nos grupos, la cual se compone de cuatro fases interrelacionadas en las que se propone la utilizaci\'on de un entorno virtual colaborativo. Se considera la inclusi\'on de un tutor humano para aquellas fases en las que las actividades grupales giren en torno a una reuni\'on virtual, as{\i}́ como un Agente Virtual Pedag\'ogico con el rol de ḷ́er del grupo durante la fase de ejecuci\'on de la tarea propuesta.},
