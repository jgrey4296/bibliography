 date-modified = {2009-03-02 20:07:58 +0100},
 issn         = {1699-4574},
 keywords     = {Entornos Virtuales Inteligentes; Entornos Virtuales Colaborativos; Sistemas Inteligentes de Tutor{\i}́a; Entrenamiento de Equipos; Agente Pedag\'ogico; Realidad Virtual.},
 number       = {2},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/31},
 volume       = {1},
}

