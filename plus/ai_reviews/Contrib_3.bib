 date-modified = {2009-03-02 19:56:57 +0100},
 issn         = {1699-4574},
 keywords     = {Hiperlectura; formaci\'on de formadores; Articulaci\'on universidad - escuela media; Educaci\'on y Nuevas Tecnolog{\i}́as; Alfabetizaci\'on Digital.},
 number       = {4},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/18},
 volume       = {2},
}

@article{IECom19,
 author       = {G\'alvez, Ram\'on Rom\'an},
 title        = {El curr{\i}́culo de la Inform\'atica en la Ley Org\'anica de Educaci\'on Espa\~nola},
 year         = {2006},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {La ense\~nanza de la Inform\'atica de la Educaci\'on Secundaria est\'a desperdigada en la asignatura de Tecnolog{\i}́a en lugar de estar definida como materia independiente, lo cual viene desencadenando los siguientes efectos: se inhabilita al profesorado de Inform\'atica -a quien no corresponde impartir la asignatura de tecnolog\'a- en sus funciones, se habilitan encubiertamente para ello a quienes no acreditan esa formaci\'on y, finalmente, se perjudica -por gusto- la calidad de su ense\~nanza. El d\'eficit y la exclusi\'on de estos conocimientos y docentes merma las posibilidades de utilizaci\'on de las Tecnolog\'ś de la Informaci\'on y la Comunicaci\'on (TIC) en todas las \'areas. As{\'\i}\'s como el Gobierno le causa, sin m\'as, un lastre al Pa{\'\i}s ́un subdesarrollo para su desenvolvimiento en esta sociedad del conocimiento del siglo XXI.},
 bdsk-url-1   = {http://www.adie.es/iecom/index.php/IECom/article/view/19},
 date-added   = {2009-03-02 19:56:11 +0100},
 date-modified = {2009-03-02 19:56:11 +0100},
 issn         = {1699-4574},
 keywords     = {Educaci\'on Secundaria; Curr{\i}́culo; Inform\'atica; Tecnolog\'as de la Informaci\'on y la Comunicaci\'on (TIC); Sociedad del Conocimiento; Ley Org\'anica de Educaci\'on (LOE).},
 number       = {4},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/19},
 volume       = {2},
}

@article{IECom20,
 author       = {D{\i}́ez, Fernando and Gil, Rafael},
 title        = {Nuevas perspectivas en resoluci\'on de problemas de matem\'aticas mediante el uso de modelos sem\'anticos de representaci\'on del conocimiento},
 year         = {2006},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {El trabajo que se presenta est\'a relacionado con el desarrollo de herramientas que permitan la interacci\'on con el alumno en sistemas de resoluci\'on de ejercicios de Matem\'aticas. Existe un d\'eficit importante de aplicaciones que fomenten y supervisen el trabajo deductivo en Matem\'aticas y que permitan explorar el nivel de aprendizaje alcanzado por un alumno en t\'erminos de razonamiento. Con el fin de mejorar la capacidad de supervisi\'on, estamos desarrollando una ontolog{\i}́a as ́como un motor de razonamiento para la representaci\'on del conocimiento en Matem\'aticas, con el fin de hacer uso de la misma mediante aplicaciones espec{\'\i}́cas (por ejemplo para realizar demostraciones sobre geometr{\'\i}.́ Se presenta el prototipo del sistema RamSys el cual integra la informaci\'on sem\'antica necesaria para la resoluci\'on de problemas de Matem\'aticas.},
 bdsk-url-1   = {http://www.adie.es/iecom/index.php/IECom/article/view/20},
 date-added   = {2009-03-02 19:57:47 +0100},
 date-modified = {2009-03-02 19:57:47 +0100},
 issn         = {1699-4574},
 keywords     = {Representaci\'on de Conocimiento; Ontolog{\i}́as; OWL; Entornos Educativos Inteligentes; Matem\'aticas.},
 number       = {4},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/20},
 volume       = {2},
}

@article{IECom21,
 author       = {Tortosa, Salvador Ot\'on and Ba{\i}́llo, Antonio Ortiz and Gonz\'alez, Jos\'e Hilera},
 title        = {SROA},
 subtitle     = {Sistema de Reutilizaci\'on de Objetos de Aprendizaje},
 year         = {2007},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {La reutilizaci\'on de contenidos docentes es una de las prioridades actuales en el mundo del elearning. De poco sirve un objeto de aprendizaje con un alto nivel de calidad, si solo es accesible por unos cuantos usuarios de una determinada plataforma o repositorio. Las instituciones educativas requieren de mecanismos de interoperabilidad, ya que no tiene sentido mantener contenido aislado en un mundo cada vez m\'as interconectado y que clama por la colaboraci\'on institucional como mecanismo para garantizar una educaci\'on de calidad. Por lo tanto, para que un objeto de aprendizaje sea reutilizable debe ser accedido desde la mayor cantidad de plataformas de e-learning posibles, as{\i}́ como por parte de todos sus clientes potenciales. Otra de las caracter\'sticas que debe tener un objeto de aprendizaje para poder ser reutilizado, es su confecci\'on conforme a est\'andares, de esta forma se asegura que, a trav\'es de su empaquetamiento y descripci\'on mediante metadatos, pueda ser integrado en cualquier plataforma de e-learning compatible con estos est\'andares. En este art\'úlo se presenta un sistema capaz de asegurar la reutilizaci\'on de los objetos de aprendizaje, haciendo interoperables los sistemas que los almacenan, de forma que la localizaci\'on de dichos objetos sea independiente de su ubicaci\'on f{\'\i}\'ca y de su tecnolog{\'\i}a\'e almacenamiento.},
 bdsk-url-1   = {http://www.adie.es/iecom/index.php/IECom/article/view/21},
 date-added   = {2009-03-02 19:51:47 +0100},
 date-modified = {2009-03-02 19:51:47 +0100},
 issn         = {1699-4574},
 keywords     = {Objetos de aprendizaje; repositorios; reutilizaci\'on; interoperabilidad.},
 number       = {5},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/21},
 volume       = {3},
}

@article{IECom22,
 author       = {Zaldivar, Vicente Romero and Arandia, Jon Elorriaga and Brito, Mateo Lezcano},
 title        = {Nuevas caracter{\i}́sticas del lenguaje OPScript para la creaci\'on de aplicaciones educativas},
 year         = {2006},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {Se presenta una breve introducci\'on al proyecto YADBrowser, el cual incluye el navegador educativo YADBrowser y su lenguaje OPScript. Se exponen algunas caracter{\i}́sticas recientemente a\~nadidas al lenguaje; con ellas el autor de una aplicaci\'on educativa puede lograr m\'as funcionalidad y adaptabilidad con menos c\'odigo. Adem\'as el navegador YADBrowser reduce la interacci\'on con el servidor, disminuyendo de esta forma el tiempo de respuesta de la aplicaci\'on. Tambi\'en puede mantener un registro de las acciones y preferencias del usuario durante la sesi\'on. Las nuevas caracter\'sticas fueron a\~nadidas para facilitar la creaci\'on de aplicaciones din\'amicas, adaptables a las habilidades del estudiante. Estas incluyen comunicaci\'on {\textquotedblleft}verbal{\textquotedblright} entre objetos, modelos de objetos basados en XML y m\'etodos reutilizables. Tambi\'en se exponen algunas caractert́́icas que pueden ser de importancia en el desarrollo de aplicaciones educativas con un componente matem\'atico.},
 bdsk-url-1   = {http://www.adie.es/iecom/index.php/IECom/article/view/22},
 date-added   = {2009-03-02 19:58:41 +0100},
 date-modified = {2009-03-02 19:58:41 +0100},
 issn         = {1699-4574},
 keywords     = {aplicaciones educativas; lenguajes de programaci\'on; adaptaci\'on; modelos de objetos; comunicaci\'on verbal.},
 number       = {4},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/22},
 volume       = {2},
}

