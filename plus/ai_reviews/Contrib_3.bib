 date-modified = {2009-03-02 20:06:35 +0100},
 issn         = {1699-4574},
 keywords     = {Evaluaci\'on basada en la web; Herramientas de aprendizaje multiling\"ues; Tests.},
 number       = {3},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/28},
 volume       = {2},
}

@article{IECom29,
 author       = {Mart{\i}́n, Bego\~na Ferrero},
 title        = {DETECTive},
 subtitle     = {Un entorno gen\'erico e integrable para diagn\'ostico de actividades de aprendizaje. Fundamentos, dise\~no y evaluaci\'on},
 year         = {2005},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {Esta tesis trata el proceso de diagn\'ostico en los sistemas educativos proponiendo un modelo de tareas y un modelo de diagn\'ostico gen\'erico aplicable a distintos dominios. Ambos modelos combinan varias t\'ecnicas para favorecer el proceso de adquisici\'on de conocimiento, y ofrecer un diagn\'ostico adecuado y fiable a partir de la revisi\'on de las respuestas de los estudiantes a los ejercicios que se les plantean. El trabajo se ha implementado en el sistema DETECTive, el cual puede utilizarse sobre diferentes dominios e integrarse con otras aplicaciones. DETECTive ha sido probado en el dominio de la m\'aquinaherramienta para construir el sistema FROGA-LAN.},
