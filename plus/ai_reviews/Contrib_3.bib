 volume       = {3},
}

@article{IECom18,
 author       = {Tessio, Noem{\i}́ and Rumbado, Florencia M. and Porro, Silvia},
 title        = {La formulaci\'on de un proyecto de integraci\'on de TICs en la formaci\'on de formadores. Hacia la hiperlectura en la acci\'on docente.},
 year         = {2006},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {Si bien la alfabetizaci\'on digital consiste en un primer paso necesario, la formaci\'on de docentes para el uso de TICs en el aula no debe agotarse all{\i}́, sino que debe aspirar a la formaci\'on de los docentes como hiperlectores y formadores de hiperlectores. En este trabajo describimos y justificamos el dise\~no de un programa de formaci\'on de docentes en Nuevas Tecnolog\'as que busca trascender la mera transmisi\'on t\'ecnica y procedimental, incorporando y promoviendo a la vez la reflexi\'on cr{\'\i}́ca sobre el uso y la incorporaci\'on social de las mismas. Asimismo abordamos algunos n\'ucleos problem\'aticos y dificultades a resolver en futuras experiencias.},
 bdsk-url-1   = {http://www.adie.es/iecom/index.php/IECom/article/view/18},
