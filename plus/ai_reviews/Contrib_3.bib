@article{IECom38,
 author       = {Pa\~neda, Xabiel G. and Neira, \'Angel and Melendi, David and Garc{\i}́a, Roberto and Garc, ́ V̧́tor G.},
 title        = {Metodolog{\i}́a para la Evaluaci\'on de un Servicio de Tele-educaci\'on Multimedia},
 year         = {2005},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {Este art{\i}́culo presenta una metodolog\'a para la evaluaci\'on de un servicio de tele-educaci\'on a trav\'es de Internet, cuyos contenidos se ofrecen al usuario en formato audio/v\'éo. A pesar de que existen diversos sistemas para hacer llegar este tipo de informaci\'on a los usuarios, la tecnolog{\'\i}d́e streaming resulta muy interesante, ya que permite transmitir a los usuarios la informaci\'on que van solicitando en tiempo real. Esta caracter{\'\i}s\'ca hace posible evaluar el \'exito de un determinado contenido, e incluso el perfil de un alumno a trav\'es de la cantidad de audio/v{\'\i}det́ransmitido, de las interacciones del usuario, y del orden de selecci\'on de los contenidos. La metodolog{\'\i}a a\'upa a diversos procesos, m\'etricas y tests para la evaluaci\'on del servicio.},
 bdsk-url-1   = {http://www.adie.es/iecom/index.php/IECom/article/view/38},
 date-added   = {2009-03-02 20:15:14 +0100},
 date-modified = {2009-03-02 20:15:14 +0100},
