 volume       = {1},
}

@article{IECom3,
 author       = {Rif\'on, Luis Anido and Santos, Juan and M{\i}́guez, Rub\'en},
 title        = {Introducci\'on de las TIC en la educaci\'on infantil de 0 a 3 a\~nos},
 year         = {2007},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {Este art{\i}́culo presenta los primeros resultados del proyecto {\textquotedblleft}galescolas.net{\textquotedblright} realizado por la Universidad de Vigo para la Secretar\'a Xeral da Igualdade de la Vicepresidencia da Igualdade e do Benestar de la Xunta de Galicia. El objetivo fundamental es iniciar el proceso de introducci\'on de las Tecnolog\'ś de la Informaci\'on y Comunicaci\'on (TIC) en la educaci\'on infantil para el rango de edades comprendido entre los 0 y los 3 a\~nos. Se realiza una introducci\'on de las principales funcionalidades ofrecidas, analizando las primeras reacciones por parte de los educadores infantiles. Asimismo se muestra la metodolog{\'\i}d́e trabajo utilizada en su desarrollo y la colaboraci\'on entre los equipos de tecn\'ologos, pedagogos y gestores de la administraci\'on aut\'onoma de Galicia.},
 bdsk-url-1   = {http://www.adie.es/iecom/index.php/IECom/article/view/3},
