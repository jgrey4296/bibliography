@inproceedings{GV-CISTI-08,
 author       = {Gonz\'alez-Moreno, Juan Carlos and V\'azquez-L\'opez, Luis},
 title        = {Generaci\'on Autom\'atica del Modelo para Sistemas Multi-Agente},
 year         = {2008},
 booktitle    = {3 Conferencia Ib\'erica de Sistemas y Tecnolog{\i}́as de la Informaci\'on (CISTI 2008)},
 tags         = {__untagged},
 abstract     = {Aunque cada vez m\'as se utilizan los sistemas multiagente, su aplicaci\'on en la metodolog{\i}́a sigue siendo escasa. Las metodolog\'as de agentes entre las que se encuentran INGENIAS, Prometheus, Tropos, PASSI, Gaia... Una de las carencias que presentan dichas metodolog\'ś, en general, es la cumplimentaci\'on de la fase de an\'alisis de una manera fiable. En cuanto a su realizaci\'on de forma autom\'atica actualmente no existen herramientas. En el proceso de an\'alisis de las necesidades y la adquisici\'on de requisitos, han sido publicados varios trabajos en los \'ultimos a\~nos. A pesar de esto, han sido escasas las herramientas que tratan de llevar a la pr\'actica los resultados obtenidos en los sistemas multiagente. El objetivo de este trabajo es presentar un m\'etodo semiautom\'atico para la realizaci\'on de la fase inicial de an\'alisis para INGENIAS a partir de la descripci\'on en lenguaje natural del problema planteado.},
 address      = {Orense, Espa\~na},
 date-added   = {2009-01-03 00:24:27 +0100},
 date-modified = {2009-08-07 09:35:21 +0200},
 isbn         = {84-612-4840-7},
 keywords     = {Itelligent Agents, AOSE, Software Engineering, Natural Language Process, INGENIAS},
 publisher    = {CISTI},
 rating       = {1},
}

