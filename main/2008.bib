@inproceedings{chambers08_unsup_learn_narrat_event_chain_31,
 author       = {Chambers, Nathanael and Jurafsky, Dan},
 title        = {Unsupervised Learning of Narrative Event Chains},
 year         = {2008},
 booktitle    = {ACL},
 tags         = {patterns,temporal,prosecution,machine_learning,metrics,narrative,employment,event_chains,partial_order,order_coherence},
 abstract     = {Essential to the selection of the next target for gaze or attention is the ability to compare the strengths of multiple competing stimuli (bottom-up information) and to signal the strongest one. Although the optic tectum (OT) has been causally implicated in stimulus selection, how it computes the strongest stimulus is unknown. Here, we demonstrate that OT neurons in the barn owl systematically encode the relative strengths of simultaneously occurring stimuli independently of sensory modality. Moreover, special {\textquotedblleft}switch-like{\textquotedblright} responses of a subset of neurons abruptly increase when the stimulus inside their receptive field becomes the strongest one. Such responses are not predicted by responses to single stimuli and, indeed, are eliminated in the absence of competitive interactions. We demonstrate that this sensory transformation substantially boosts the representation of the strongest stimulus by creating a binary discrimination signal, thereby setting the stage for potential winner-take-all target selection for gaze and attention.},
 doi          = {10.1.1.143.1555},
 file         = {2008/Chambers/Chambers_2008_Unsupervised_Learning_of_Narrative_Event.pdf},
 isbn         = {9781932432046},
 number       = {14},
 pages        = {789{\textendash}797},
 url          = {http://acl.eldoc.ub.rug.nl/mirror/P/P08/P08-1090.pdf},
 volume       = {31},
}

