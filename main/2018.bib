@article{seaver18_captiv_algor_24,
 author       = {Seaver, Nick},
 title        = {Captivating Algorithms},
 subtitle     = {Recommender Systems As Traps},
 year         = {2018},
 journal      = {Journal of Material Culture},
 tags         = {anthropology,surveillance,online,captivation_metrics,traps,ethics,algorithm,recommendation,metrics,culture},
 abstract     = {Algorithmic recommender systems are a ubiquitous feature of contemporary cultural life online, suggesting music, movies, and other materials to their users. This article, drawing on fieldwork with developers of recommender systems in the US, describes a tendency among these systems' makers to describe their purpose as {\textquotedblleft}hooking{\textquotedblright} people-enticing them into frequent or enduring usage. Inspired by steady references to capture in the field, I consider recommender systems as traps, drawing on anthropological theories about animal trapping. I chart the rise of {\textquotedblleft}captivation metrics{\textquotedblright}-measures of user retention-enabled by a set of transformations in recommenders' epistemic, economic, and technical contexts. Traps prove useful for thinking about how such systems relate to broader infrastructural ecologies of knowledge and technology. As recommenders spread across online cultural infrastructures and become practically inescapable, thinking with traps offers an alternative to common ethical framings that oppose tropes of freedom and coercion.},
 doi          = {10.1177/1359183518820366},
 file         = {2018/Seaver/Seaver_2018_Captivating_Algorithms.pdf},
 issn         = {1359-1835},
 number       = {4},
