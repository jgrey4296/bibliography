@article{sayer17_white_riot_brexit_trump_post_factual_polit_30,
 author       = {Sayer, Derek},
 title        = {White Riot-Brexit, Trump, and Post-Factual Politics},
 year         = {2017},
 journal      = {Journal of Historical Sociology},
 tags         = {brexit,politics,trump,white_riot,race},
 abstract     = {The term {\textquotedblleft}post-factual politics{\textquotedblright} (or {\textquotedblleft}post-truth politics'), which Wikipedia usefully defines as {\textquotedblleft}a political culture in which debate is framed largely by appeals to emotion disconnected from the details of policy, and by the repeated assertion of talking points to which factual rebuttals are ignored,{\textquotedblright} has been widely employed in relation to Britain's EU referendum and the 2016 US presidential election. At issue here are not just particular lies, like the 350 million per week Leave campaigners in the UK falsely claimed went to the EU, but the whole spirit in which the debates were conducted. Warnings of negative economic consequences of a pro-Brexit vote were routinely dismissed as {\textquotedblright} Project Fear {\textquotedblright} -the implication being that is was unpatriotic even to question Britain's ability to prosper outside the EU. A will-ingness to ignore facts came close to being presented as a moral virtue. Michael Gove encapsulated this spirit in his claim that {\textquotedblright} people in this country have had enough of experts {\textquotedblright} -a remarkable statement to come from a former Sec-retary of State for Education. 1 On the other side of the Atlantic Donald Trump's contempt for facts scarcely needs documenting, but those interested might consult the Toronto Star's list of 560 falsehoods he uttered during the cam-paign on every conceivable topic from youth unemployment rates among African-Americans to the numbers of Syrian refugees admitted to the USA by the Obama administration. 2 On Trump's first day in office White House press sec-retary Sean Spicer berated the press for understating the size of the crowds at the inauguration, insisting that this was {\textquotedblright} the largest audience to ever witness an inauguration, period. {\textquotedblright} 3 When challenged with evidence that clearly showed the contrary, presidential advisor Kellyanne Conway described Spicer's falsehoods as {\textquotedblright} alternative facts. {\textquotedblright} 4 Such abuses of truth by the powerful do not augur well for the future of democracy, which depends on an informed electorate. My concern here, however, is not primarily with Westminster and the White House. It is with the extent to which left- wing analysis of the Brexit and Trump victories has become clouded by the same post-factual fog. Perception of these victories as {\textquotedblright} upsets {\textquotedblright} or {\textquotedblright} shocks {\textquotedblright} is arguably itself a symptom of a political culture in which feeling and belief count for more than evidence or argument. No rational observer should have been surprised by these outcomes. In the British case, though most polls published during the last week of the campaign predicted a slender win for Remain, over the previous three weeks 17 out of 26 polls had predicted a Leave victory. 5 In the US, where most polls had shown a majority for Clinton for months, the gap sharply narrowed in the week before the election, leaving her with a bare 3.1 },
 doi          = {10.1111/johs.12153},
 file         = {2017/Sayer/Sayer_2017_White_Riot_Brexit_Trump_and_Post_Factu.pdf},
 issn         = {14676443},
 number       = {1},
 pages        = {92{\textendash}106},
 volume       = {30},
}

