@article{hallett06_inhab_instit_35,
 author       = {Hallett, Tim and Ventresca, Marc J.},
 title        = {Inhabited Institutions},
 subtitle     = {Social Interactions and Organizational Forms in Gouldner's Patterns of Industrial Bureaucracy},
 year         = {2006},
 journal      = {Theory and Society},
 tags         = {inhabited_institutions,social_interaction,embeddedness,history,gypsum_mine,symbolic_interaction,bureaucracy,alvin_gouldner,field,organisation,__printed},
 abstract     = {Organizational sociologists often treat institutions as macro cultural logics, representations, and schemata, with less consideration for how institutions are {\textquotedblleft}inhabited{\textquotedblright} (Scully and Creed, 1997) by people doing things together. As such, this article uses a symbolic interactionist rereading of Gouldner's classic study Patterns of Industrial Bureaucracy as a lever to expand the boundaries of institutionalism to encompass a richer understanding of action, interaction, and meaning. Fifty years after its publication, Gouldner's study still speaks to us, though in ways we (and he) may not have anticipated five decades ago. The rich field observations in Patterns remind us that institutions such as bureaucracy are inhabited by people and their interactions, and the book provides an opportunity for intellectual renewal. Instead of treating contemporary institutionalism and symbolic interaction as antagonistic, we treat them as complementary components of an {\textquotedblleft}inhabited institutions approach{\textquotedblright} that focuses on local and extra-local embeddedness, local and extra-local meaning, and a skeptical, inquiring attitude. This approach yields a doubly constructed view: On the one hand, institutions provide the raw materials and guidelines for social interactions ({\textquotedblright}construct interactions{\textquotedblright}), and on the other hand, the meanings of institutions are constructed and propelled forward by social interactions. Institutions are not inert categories of meaning; rather they are populated with people whose social interactions suffuse institutions with local force and significance.},
 doi          = {10.1007/s11186-006-9003-z},
 file         = {2006/Hallett/Hallett_2006_Inhabited_Institutions.pdf},
 issn         = {03042421},
 number       = {2},
 pages        = {213{\textendash}236},
 volume       = {35},
}

