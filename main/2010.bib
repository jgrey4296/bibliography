@article{dominus10_short_introd_to_topol_14,
 author       = {Dominus, Mark Jason},
 title        = {Short Introduction To Topology},
 year         = {2010},
 journal      = {Topology},
 tags         = {topology,math,sets},
 abstract     = {Topology is the branch of mathematics that tries to understand continuity and continuous functions. You probably don't need to know much topology for your CS degree, but people will bring it up from time to time. For example, you will hear in your category theory class that topological spaces form a category with continuous functions as the arrows. Or you will hear that certain topological spaces are natural models for intuitionistic logic. The goal of these notes is to be the minimal explanation of topology that will enable you to understand those two things. On the real line, continuity is defined in terms of distance: a function from R to R is continuous if points that were sufficiently close together in the domain are mapped to points that are close together in the codomain. The use of {\textquotedblleft}close together{\textquotedblright} implies a distance function, which in R is simply |x - y|. But what about spaces that are more complex, for which distance doesn't make sense, or where no distance function is apparent? Topology reformulates this idea of {\textquotedblright}closeness{\textquotedblright} in terms of special sets called {\textquotedblright}open sets{\textquotedblright}.},
 file         = {2010/Dominus/Dominus_2010_Short_Introduction_To_Topology.pdf},
 isbn         = {0486406806},
 pages        = {1{\textendash}11},
 url          = {http://pic.blog.plover.com/math/topology-doc/topology-draft-0.6.pdf},
 volume       = {14},
}

