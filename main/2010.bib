@article{de10_indig_cosmop_andes_25,
 author       = {De La Cadena, Marisol},
 title        = {Indigenous Cosmopolitics in the Andes},
 subtitle     = {Conceptual Reflections Beyond {\textquotedblleft}Politics{\textquotedblright}},
 year         = {2010},
 journal      = {Cultural Anthropology},
 tags         = {andes,politics,latin_america,anti_mining,pluriverse,cosmopolitics,indigenous,culture},
 abstract     = {In Latin America indigenous politics has been branded as {\textquotedblleft}ethnic politics.{\textquotedblright} Its activism is interpreted as a quest to make cultural rights prevail. Yet, what if {\textquotedblright}culture{\textquotedblright} is insufficient, even an inadequate notion, to think the challenge that indigenous politics represents? Drawing inspiration from recent political events in Peru-and to a lesser extent in Ecuador and Bolivia-where the indigenous-popular movement has conjured sentient entities (mountains, water, and soil-what we call {\textquotedblright}nature{\textquotedblright}) into the public political arena, the argument in this essay is threefold. First, indigeneity, as a historical formation, exceeds the notion of politics as usual, that is, an arena populated by rational human beings disputing the power to represent others vis-\`a-vis the state. Second, indigeneity's current political emergence-in oppositional antimining movements in Peru and Ecuador, but also in celebratory events in Bolivia-challenges the separation of nature and culture that underpins the prevalent notion of politics and its according social contract. Third, beyond {\textquotedblright}ethnic politics{\textquotedblright} current indigenous movements, propose a different political practice, plural not because of its enactment by bodies marked by gender, race, ethnicity or sexuality (as multiculturalism would have it), but because they conjure nonhumans as actors in the political arena.},
 doi          = {10.1111/j.1548-1360.2010.01061.x},
 file         = {2010/De/De_2010_Indigenous_Cosmopolitics_in_the_Andes.pdf},
 issn         = {08867356},
 number       = {2},
 pages        = {334{\textendash}370},
 volume       = {25},
}

