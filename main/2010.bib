@inproceedings{mcgee10_real_time_team_mate_ai_games_,
 author       = {Mcgee, Kevin and Abraham, Aswin Thomas},
 title        = {Real-Time Team-Mate Ai in Games},
 subtitle     = {A Definition, Survey, \& Critique},
 year         = {2010},
 booktitle    = {FDG '10 Proceedings of the Fifth International Conference on the Foundations of Digital Games},
 tags         = {teams,agents,review,ai},
 abstract     = {Many contemporary games are team-based and there is a growing interest in, and need for, advances in team-mate AI for games. However, although there have been surveys of agent AI in games, to date there has been no survey of work on team-mate AI. Furthermore, the concept of {\textquotedblleft}team-mate AI{\textquotedblright} is not currently well delineated to distinguish between work on independently-acting agents that happen to be on the same side from work on agents the coordinate their behaviors and decision-making in terms of their teammates behaviors, intentions, and the like. Also, it is important to distinguish between game AI that is used as an optimization technique from real-time game AI, so this paper proposes a definition for real-time team-mate AI (highlighted with examples by game genre), reviews work to date to implement real-time team-mate AI for games in terms of a number of AI research areas (e.g., coordinated action, prediction, learning), and concludes with a brief discussion of significant issues about the state of the art.},
 doi          = {10.1145/1822348.1822365},
 file         = {2010/Mcgee/Mcgee_2010_Real_Time_Team_Mate_Ai_in_Games.pdf},
